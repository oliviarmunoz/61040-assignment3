{
  "name": "gemini-schedule-demo",
  "version": "1.0.0",
  "description": "A demo application showing how to use Google's Gemini API to create student schedules",
  "main": "dist/recommend-tests.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/recommend-tests.js",
    "dev": "ts-node --transpile-only recommend-tests.ts",
    "test": "npm run build && node dist/recommend-tests.js",
    "manual": "npm run build && node -e \"require('./dist/recommend-tests.js').testManualScheduling()\"",
    "llm": "npm run build && node -e \"require('./dist/recommend-tests.js').testLLMScheduling()\"",
    "mixed": "npm run build && node -e \"require('./dist/recommend-tests.js').testMixedScheduling()\""
  },
  "keywords": ["gemini", "ai", "schedule", "student", "demo", "typescript"],
  "author": "Olivia Munoz",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.21.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "ts-node": "^10.9.0"
  }
}
